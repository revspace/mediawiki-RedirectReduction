{
	"name": "RedirectReduction",
	"version": "1.0",
	"author": [
		"Shiz"
	],
	"url": "https://github.com/revspace/mediawiki-RedirectReduction",
	"descriptionmsg": "ext.redirectreduction.desc",
	"license-name": "GPL-2.0",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},

	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\RedirectReduction\\" : "src/"
	},
	"MessagesDirs": {
		"RedirectReduction": [
			"i18n"
		]
	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\RedirectReduction\\Hooks"
		}
	},
	"Hooks": {
		"TestCanonicalRedirect": "main"
	},

	"load_composer_autoloader" : true,
	"manifest_version": 2
}
